<h1>Sign Up for your FREE Viewing Party PRO Account TODAY!</h1>

<%= form_with url: "/users", method: :post, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :password %>
  <%= f.text_field :password %>
  <%= f.label :password_confirmation, "Confirm Password" %>
  <%= f.text_field :password_confirmation %>
  <%= f.submit "Register" %>
<% end %>
